*
      margin 0
      padding 0
      box-sizing border-box
      font-family Poppins

.description
      max-width 1920px
      width 100%

      margin 0 auto

      &__container 
            max-width 1920px
            width 100%

            &-values 
                  max-width 1920px
                  width 100%

                  padding 64px 32px
                  margin-bottom 96px

                  background #232323

                  h2
                        text-align center
                        text-transform uppercase

                        margin-bottom 16px
                        
                        font-style normal
                        font-weight 400
                        font-size 38px
                        line-height 46px
                        
                        color #FAFAFA

                  ul
                        display flex

                        max-width 1856px
                        width 100%

                        justify-content space-between
                        align-items center

                  li 
                        max-width 440px
                        width 100%

                        padding 16px

                        list-style none
                  
                  svg 
                        margin-bottom 16px

                  h3
                        text-transform uppercase

                        margin-bottom 16px

                        font-style normal
                        font-weight normal
                        font-size 20px
                        line-height 28px

                        color #FAFAFA

                  p 
                        font-style normal
                        font-weight normal
                        font-size 16px
                        line-height 24px
                        
                        color #B0B0B0

                  .white
                        text-align center
                        
                        margin-bottom 64px

                        font-style normal
                        font-weight normal
                        font-size 20px
                        line-height 28px
                        
                        color #FAFAFA 

            &-title
                  max-width 1400px
                  width 100%

                  text-align center 

                  margin 0 auto 48px auto

                  h2
                        text-align center
                        text-transform uppercase

                        margin-bottom 16px
                        
                        font-style normal
                        font-weight 700
                        font-size 38px
                        line-height 46px
                        
                        color #000000

                  p 
                        font-style normal
                        font-weight normal
                        font-size 20px
                        line-height 28px
                        
                        color #595959

            &-info1
                  max-width 1400px
                  width 100%

                  margin 48px auto

            &-show1, &-show2, &-show3
                  max-width 1400px
                  width 100%

                  display flex 
                  justify-content space-between

                  margin-bottom 48px

            &-main 
                  max-width 684px
                  width 100%

                  h3
                        text-transform uppercase

                        margin-bottom 16px

                        font-style normal
                        font-weight 700
                        font-size 24px
                        line-height 30px

                        color #000000
                  
                  p 
                        font-style normal
                        font-weight normal
                        font-size 20px
                        line-height 28px
                        
                        color #595959
                  
            &-comment1
                  max-width 1400px
                  width 100%

                  padding 56px

                  background #FAFAFA

                  border-radius 12px

                  svg 
                        margin-bottom 32px

                  h3 
                        margin-bottom 16px

                        text-transform uppercase

                        font-style normal
                        font-weight 700
                        font-size 30px
                        line-height 38px

                        color #000000

                  p 
                        font-style normal
                        font-weight normal
                        font-size 20px
                        line-height 28px
                        
                        color #595959

            &-info2
                  max-width 1400px
                  width 100%

                  margin 48px auto

                  svg 
                        order 2 !important
            
            &-comment2
                  max-width 1400px
                  width 100%

                  background #FFF

                  h3 
                        margin-bottom 8px

                        text-transform uppercase

                        font-style normal
                        font-weight 700
                        font-size 24px
                        line-height 32px

                        color #000000

                  p 
                        font-style normal
                        font-weight normal
                        font-size 20px
                        line-height 28px
                        
                        color #595959

            &-info3
                  max-width 1400px
                  width 100%

                  margin 48px auto 88px auto

                  ul 
                        li 
                              margin-bottom 16px

                              font-style normal
                              font-weight normal
                              font-size 20px
                              line-height 28px

                              color #595959

                              list-style inside
                  

// ------- MEDIA QUERIES -------   

@media only screen and (min-width: 991px) and (max-width 1439px)

      .description
            &__container
                  &-values
                        margin-bottom 96px
                        padding 32px 16px 16px

                        h2
                              text-align center
                        
                        .white
                              text-align center
                        
                        ul 
                              flex-wrap wrap  
                        
                        li 
                              max-width 470px
                            
                              margin 0 auto
                                      
                  &-info1, &-info2, &-info3
                        padding 0 15px
                        max-width 70%

                        div
                              max-width 100%
                              min-width 684px
                              margin 32px auto
            
                  &-show1, &-show3
                        flex-wrap wrap 
                        justify-content center

                        svg 
                              order 0
                              margin-bottom 32px

                  &-show2 
                        flex-wrap wrap 
                        justify-content center

                        svg 
                              order 0 !important
                              margin-bottom 32px

